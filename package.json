{
  "name": "with-chakra-ui-typescript",
  "version": "1.0.0",
  "engines": {
    "node": "14.15.3"
  },
  "scripts": {
    "dev": "run-p dev:*",
    "dev:main": "next",
    "dev:storybook": "start-storybook -p 6006",
    "build": "run-p build:*",
    "build:main": "next build",
    "build:storybook": "build-storybook",
    "start": "next start",
    "pre-commit": "run-s lint-staged validate",
    "lint-staged": "lint-staged",
    "validate": "run-p validate:*",
    "validate:type": "tsc --noEmit",
    "validate:lint": "eslint \"src/**/*.{ts,tsx}\"",
    "validate:format": "prettier -c \"src/**/*.{ts,tsx}\"",
    "lint:fix": "eslint src/**/*.ts --fix",
    "prepare": "husky install",
    "add-component": "hygen generator add-component"
  },
  "dependencies": {
    "@chakra-ui/icons": "^1.0.5",
    "@chakra-ui/react": "^1.4.2",
    "@chakra-ui/theme-tools": "1.1.2",
    "@emotion/react": "11.1.5",
    "@emotion/styled": "11.1.5",
    "framer-motion": "^4.0.3",
    "next": "latest",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@storybook/addon-a11y": "^6.2.9",
    "@storybook/addon-actions": "^6.2.9",
    "@storybook/addon-docs": "^6.2.9",
    "@storybook/addon-knobs": "^6.2.9",
    "@storybook/addon-links": "^6.2.9",
    "@storybook/addon-storyshots": "^6.2.9",
    "@storybook/addons": "^6.2.9",
    "@storybook/react": "^6.2.9",
    "@types/node": "^14.6.0",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "@typescript-eslint/eslint-plugin": "^4.24.0",
    "@typescript-eslint/parser": "^4.24.0",
    "babel-plugin-react-require": "^3.1.3",
    "babel-preset-react-app": "^10.0.0",
    "eslint": "^7.27.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.23.3",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^6.0.0",
    "hygen": "^6.1.0",
    "lint-staged": "^11.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.3.0",
    "storybook-addon-jsx": "^7.3.10",
    "typescript": "4.2.3"
  },
  "license": "MIT",
  "lint-staged": {
    "./src/**/*.{ts,tsx}": [
      "prettier -w"
    ]
  }
}
